{
  "name": "production-react-advanced-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": " webpack serve --env port=3005",
    "start:vite": "vite",
    "start:dev": "concurrently \"npm run start\" \"npm run start:dev:server\"",
    "start:dev:vite": "concurrently \"npm run start:vite\" \"npm run start:dev:server\"",
    "start:dev:server": "node ./json-server/index.js",
    "build:prod": "webpack --env mode=production apiUrl=https://production-react-advanced-project-backend-7o9eap14b.vercel.app",
    "build:prod:vite": "vite build",
    "build:dev": "webpack --env mode=development",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:cypress": "cypress open",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki test approve",
    "test:ui:ci": "loki --requireReference --reactUri file:./storybook-static",
    "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook",
    "export-svg-icons": "node ./scripts/export-svg-icons.js",
    "export-ui-components": "node ./scripts/export-ui-components.js",
    "generate:layer": "node ./scripts/create-slice/index.js",
    "update-imports": "ts-node ./scripts/refactoring/updateImports.ts",
    "create-public-shared-api": "ts-node ./scripts/refactoring/createPublicApiForSharedUI.ts",
    "update-eslint-plugin": "npm i -D eslint-plugin-nothingg9537-plugin@latest",
    "postinstall": "rimraf ./node_modules/.cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nothing9537/production-react-advanced-project.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nothing9537/production-react-advanced-project/issues"
  },
  "homepage": "https://github.com/nothing9537/production-react-advanced-project#readme",
  "lint-staged": {
    "**/*.{ts,tsx}": "eslint"
  },
  "overrides": {
    "loki": {
      "@storybook/react": "$@storybook/react"
    }
  },
